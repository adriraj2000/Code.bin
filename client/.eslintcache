err.response.data.msg &&\r\n                setUser({ ...user, err: err.response.data.msg, success: '' })\r\n        }\r\n    }\r\n\r\n    const responseGoogle = async (response) => {\r\n        try {\r\n            const res = await axios.post('/user/google_login', { tokenId: response.tokenId })\r\n\r\n            setUser({ ...user, error: '', success: res.data.msg })\r\n            localStorage.setItem('firstLogin', true)\r\n\r\n            dispatch(dispatchLogin())\r\n            history.push('/')\r\n        } catch (err) {\r\n            err.response.data.msg &&\r\n                setUser({ ...user, err: err.response.data.msg, success: '' })\r\n        }\r\n    }\r\n\r\n\r\n    return (\r\n        <div className=\"login_page\">\r\n            <h2>Login</h2>\r\n            {err && showErrMsg(err)}\r\n            {success && showSuccessMsg(success)}\r\n\r\n            <form onSubmit={handleSubmit}>\r\n                <div>\r\n                    <label htmlFor=\"email\">Email Address</label>\r\n                    <input type=\"text\" placeholder=\"Enter email address\" id=\"email\"\r\n                        value={email} name=\"email\" onChange={handleChangeInput} />\r\n                </div>\r\n\r\n                <div>\r\n                    <label htmlFor=\"password\">Password</label>\r\n                    <input type=\"password\" placeholder=\"Enter password\" id=\"password\"\r\n                        value={password} name=\"password\" onChange={handleChangeInput} />\r\n                </div>\r\n\r\n                <div className=\"row\">\r\n                    <button type=\"submit\">Login</button>\r\n                    <Link to=\"/forgot_password\">Forgot your password?</Link>\r\n                </div>\r\n            </form>\r\n\r\n            <div className=\"hr\">Or Login With</div>\r\n            <div className=\"social\">\r\n                <GoogleLogin\r\n                    clientId=\"681171991249-mcs2d4o4qkc63qguse7f4jddroos28oo.apps.googleusercontent.com\"\r\n                    buttonText=\"Login with google\"\r\n                    onSuccess={responseGoogle}\r\n                    cookiePolicy={'single_host_origin'}\r\n                />\r\n                <GitHubLogin clientId=\"your github client id\"\r\n                    onSuccess={onSuccess}\r\n                    onFailure={onFailure} />\r\n            </div>\r\n\r\n            <p>New Customer? <Link to=\"/register\">Register</Link></p>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Login","C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\utils\\notification\\notification.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\body\\auth\\Register.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\body\\auth\\ActivationEmail.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\utils\\validation\\validation.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\body\\auth\\ForgotPassword.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\utils\\NotFound\\NotFound.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\body\\auth\\ResetPassword.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\body\\profile\\Profile.js",["107","108","109"],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\body\\home\\Home.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\body\\profile\\EditUser.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\utils\\notification\\Notification.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop\\Code.bin\\Code.bin\\client\\src\\components\\utils\\validation\\Validation.js",[],"C:\\Users\\Adriraj Chaudhuri\\Desktop